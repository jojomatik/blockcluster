{
  "name": "blockcluster-frontend",
  "version": "0.3.0",
  "private": true,
  "scripts": {
    "install_all": "concurrently \"npm install\" \"cd backend && npm install\" \"cd proxy && npm install\"",
    "serve": "concurrently \"vue-cli-service serve\" \"cd backend && npm start\" \"cd proxy && npm start\"",
    "build": "vue-cli-service build",
    "build_all": "concurrently \"vue-cli-service build\" \"cd backend && tsc\" \"cd proxy && tsc\"",
    "start": "concurrently \"http-server dist -p 8080\" \"cd backend && node dist/backend/src/backend.js\" \"cd proxy && node dist/proxy.js\"",
    "test:unit": "vue-cli-service test:unit",
    "lint": "vue-cli-service lint"
  },
  "dependencies": {
    "apexcharts": "^3.26.0",
    "concurrently": "^6.0.0",
    "core-js": "^3.10.0",
    "moment": "^2.29.1",
    "socket.io-client": "^4.0.1",
    "uuid": "^8.3.2",
    "vue": "^2.6.11",
    "vue-apexcharts": "^1.6.0",
    "vue-class-component": "^7.2.3",
    "vue-meta": "^2.4.0",
    "vue-property-decorator": "^9.1.2",
    "vue-router": "^3.2.0",
    "vue-socket.io": "^3.0.10",
    "vuetify": "^2.4.9",
    "vuex": "^3.4.0"
  },
  "devDependencies": {
    "@mdi/font": "^5.9.55",
    "@types/jest": "^26.0.22",
    "@types/uuid": "^8.3.0",
    "@typescript-eslint/eslint-plugin": "^2.33.0",
    "@typescript-eslint/parser": "^2.33.0",
    "@vue/cli-plugin-babel": "~4.5.12",
    "@vue/cli-plugin-eslint": "~4.5.12",
    "@vue/cli-plugin-router": "~4.5.12",
    "@vue/cli-plugin-typescript": "~4.5.12",
    "@vue/cli-plugin-unit-jest": "~4.5.12",
    "@vue/cli-plugin-vuex": "~4.5.12",
    "@vue/cli-service": "~4.5.12",
    "@vue/eslint-config-prettier": "^6.0.0",
    "@vue/eslint-config-typescript": "^5.0.2",
    "@vue/test-utils": "^1.1.3",
    "eslint": "^6.7.2",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-vue": "^7.9.0",
    "prettier": "^2.2.1",
    "sass": "^1.32.8",
    "sass-loader": "^10.0.0",
    "socket.io": "^4.0.1",
    "typescript": "~4.2.3",
    "vue-cli-plugin-vuetify": "~2.3.1",
    "vue-template-compiler": "^2.6.11",
    "vuetify-loader": "^1.7.0"
  }
